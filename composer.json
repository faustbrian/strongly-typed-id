{
    "name": "cline/strongly-typed-id",
    "description": "Strongly-typed ID value objects for PHP with support for UUID, ULID, NanoID, GUID, Sqid, Hashids and Laravel integration",
    "keywords": [
        "strongly-typed",
        "value-object",
        "identifier",
        "uuid",
        "ulid",
        "nanoid",
        "guid",
        "sqid",
        "hashids",
        "laravel",
        "eloquent",
        "ddd",
        "domain-driven-design",
        "type-safety"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require": {
        "php": "^8.4.0",
        "spatie/laravel-data": "^4.17.1",
        "spatie/laravel-package-tools": "^1.92.7",
        "sqids/sqids": "^0.5.0",
        "hashids/hashids": "^5.0"
    },
    "require-dev": {
        "cline/php-cs-fixer": "^1.0.4",
        "driftingly/rector-laravel": "^2.0.7",
        "ergebnis/composer-normalize": "^2.49@dev",
        "laravel/pint": "^1.25.1",
        "orchestra/testbench": "^10.6",
        "pestphp/pest": "^3.8.4",
        "pestphp/pest-plugin-laravel": "^3.2",
        "pestphp/pest-plugin-type-coverage": "^3.6.1",
        "phpstan/phpstan": "^2.1.31",
        "rector/rector": "^2.2.2",
        "symfony/var-dumper": "^7.3.4"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cline\\StronglyTypedId\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cline\\StronglyTypedId\\StronglyTypedIdServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": "vendor/bin/php-cs-fixer fix",
        "refactor": "rector",
        "test": [
            "@test:lint",
            "@test:type-coverage",
            "@test:unit",
            "@test:types",
            "@test:refactor"
        ],
        "test:lint": "vendor/bin/php-cs-fixer check --diff",
        "test:refactor": "rector --dry-run",
        "test:type-coverage": "pest --parallel --exactly=100 --type-coverage",
        "test:types": "phpstan",
        "test:unit": "pest --parallel --exactly=100 --coverage"
    }
}
