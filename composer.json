{
  "name": "cline/strongly-typed-id",
  "description": "Strongly-typed ID value objects for PHP with support for UUID, ULID, NanoID, GUID, Sqid, Hashids and Laravel integration",
  "license": "MIT",
  "keywords": [
    "strongly-typed",
    "value-object",
    "identifier",
    "uuid",
    "ulid",
    "nanoid",
    "guid",
    "sqid",
    "hashids",
    "laravel",
    "eloquent",
    "ddd",
    "domain-driven-design",
    "type-safety"
  ],
  "authors": [
    {
      "name": "Brian Faust",
      "email": "brian@cline.sh"
    }
  ],
  "require": {
    "php": "^8.5.0",
    "hashids/hashids": "^5.0.2",
    "spatie/laravel-data": "^4.18.0",
    "spatie/laravel-package-tools": "^1.92.7",
    "sqids/sqids": "^0.5.0"
  },
  "require-dev": {
    "cline/coding-standard": "^1.5.7",
    "ergebnis/composer-normalize": "^2.48.2",
    "orchestra/testbench": "^10.8",
    "pestphp/pest": "^4.1.6",
    "pestphp/pest-plugin-laravel": "^4.0",
    "pestphp/pest-plugin-type-coverage": "^4.0.3",
    "symfony/var-dumper": "^7.4.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Cline\\StronglyTypedId\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cline\\StronglyTypedId\\StronglyTypedIdServiceProvider"
      ]
    }
  },
  "scripts": {
    "lint": "ecs check --fix",
    "refactor": "rector",
    "test": [
      "@test:lint",
      "@test:type-coverage",
      "@test:unit",
      "@test:types",
      "@test:refactor"
    ],
    "test:lint": "ecs check",
    "test:refactor": "rector --dry-run",
    "test:type-coverage": "pest --parallel --min=90 --type-coverage",
    "test:types": "phpstan",
    "test:unit": "pest --parallel --min=90 --coverage"
  }
}
