{
    "name": "cline/strongly-typed-id",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-data": "^4.17.1",
        "spatie/laravel-package-tools": "^1.92.7"
    },
    "require-dev": {
        "cline/php-cs-fixer": "^1.0",
        "driftingly/rector-laravel": "^2.0.7",
        "ergebnis/composer-normalize": "^2.48.2",
        "larastan/larastan": ">=3.7.2",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.1.1",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": ">=1.4.3",
        "phpstan/phpstan": ">=2.1.30",
        "phpstan/phpstan-strict-rules": "^2.0.7",
        "rector/rector": "^2.1.7"
    },
    "autoload": {
        "psr-4": {
            "Cline\\StronglyTypedId\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cline\\StronglyTypedId\\StronglyTypedIdServiceProvider"
            ]
        }
    }
}
